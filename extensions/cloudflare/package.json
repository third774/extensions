{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cloudflare",
  "title": "Cloudflare",
  "description": "Search and view your Cloudflare sites, pages, domains, and more",
  "icon": "icon.png",
  "author": "destiner",
  "categories": [
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "contributors": [
    "niklaswa"
  ],
  "commands": [
    {
      "name": "view-sites",
      "title": "View Sites",
      "subtitle": "Cloudflare",
      "description": "Lists your sites",
      "mode": "view"
    },
    {
      "name": "view-pages",
      "title": "View Pages",
      "subtitle": "Cloudflare",
      "description": "Lists your page projects",
      "mode": "view"
    },
    {
      "name": "view-members",
      "title": "View Members",
      "subtitle": "Cloudflare",
      "description": "Lists your team members",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "API Token",
      "description": "API token",
      "placeholder": "Your token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.0",
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.27",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "typecheck": "tsc --noEmit"
  }
}
